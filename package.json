{
    "name": "tarkov-quest-tracker",
    "version": "1.0.0",
    "description": "Interactive static SPA for tracking Escape from Tarkov quests with path optimization to Lightkeeper, Setup, and Test Drive",
    "main": "src/index.js",
    "scripts": {
        "start": "npx http-server . -p 8080 -o",
        "dev": "npx http-server . -p 8080 -c-1",
        "fetch-data": "node scripts/fetch-quest-data.js",
        "build": "npm run fetch-data"
    },
    "keywords": [
        "tarkov",
        "escape-from-tarkov",
        "quest-tracker",
        "spa"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "http-server": "^14.1.1"
    },
    "dependencies": {
        "cytoscape": "^3.28.1",
        "cytoscape-dagre": "^2.5.0"
    }
}